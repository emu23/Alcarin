<section id="game-events">
    <div class="events">
        <div class="events-container">
            <ul>
                {% for i in 0..8 %}
                <li>Something strange just happen..</li>
                {% endfor %}
            </ul>
        </div>
    </div>
</section>

<div id="main-nav">
    <nav>
        <ul class="nav-bar">
            {% for class in ['home', 'glass', 'plane', 'eye-close', 'circle-arrow-up', 'calendar'] %}
            <li>
                <a href="#{{class}}">
                    <i class="icon-{{class}}"></i>
                </a>
            </li>
            {% endfor %}
        </ul>
    </nav>
</div>

<section id="game-pages">
    <div class="pages">
        <div class="pages-container">
            <div class="page-home current">
                {% for i in 0..118 %} home.. {% endfor %}
            </div>
            {% for class in ['home', 'glass', 'plane', 'eye-close', 'circle-arrow-up', 'calendar'] %}
            <div class="page-{{class}} hide">
                {% for i in 0..118 %} {{class}}.. {% endfor %}
            </div>
            {% endfor %}
        </div>
    </div>
</section>

<div style="clear: both"></div>